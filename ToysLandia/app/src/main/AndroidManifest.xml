<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.equipo.musume.toyslandia">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".IniciarActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".InicioActivity"
            android:label="@string/title_activity_inicio"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ScannerActivity"
            android:label="@string/title_activity_scanner"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MostrarItemActivity"
            android:label="@string/title_activity_mostrar_item"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".PedirItemActivity"
            android:label="@string/title_activity_pedir_item"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>